{
  "version": 2,
  "builds": [
    {
      "src": "vercel-app-supabase.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel-app-supabase.js"
    }
  ],
  "env": {
    "PORT": "3001",
    "SUPABASE_URL": "${SUPABASE_URL}",
    "SUPABASE_KEY": "${SUPABASE_KEY}",
    "OPENAI_API_KEY": "${OPENAI_API_KEY}"
  }
}
