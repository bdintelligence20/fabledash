{
  "version": 2,
  "builds": [
    {
      "src": "vercel-app-supabase.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel-app-supabase.js"
    }
  ],
  "env": {
    "PORT": "3001",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
